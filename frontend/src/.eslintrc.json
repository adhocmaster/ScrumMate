{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard-with-typescript",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
		"no-unused-vars": "warn",
		"react/prop-types": "warn",
		"eqeqeq": "warn",
		"multiline-ternary": "warn",
		"no-tabs": "warn",
		"no-mixed-spaces-and-tabs": "warn",
    }
}
